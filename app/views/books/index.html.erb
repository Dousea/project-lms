<h1>Listing Books</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Publisher</th>
    <th></th>
  </tr>
 
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.publisher.name %></td>
      <td><%= link_to 'Show', book_path(book) %></td>
    </tr>
  <% end %>
</table>

<%= paginate @books %>