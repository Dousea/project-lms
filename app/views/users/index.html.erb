<h1>Listing Users</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Role</th>
    <th></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.role %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>

<%= paginate @users %>